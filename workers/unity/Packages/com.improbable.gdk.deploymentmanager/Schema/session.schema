    LOBBY = 1;
    RUNNING = 2;
    STOPPING = 3;
    STOPPED = 4;
} 

component Session {
    id = 20000;
    Status status = 1;
}

component Deployment {
    id = 20001;
}

component Timer {
    id = 20002;
    uint32 current_time_seconds = 1;
    uint32 max_time_seconds = 2;
}